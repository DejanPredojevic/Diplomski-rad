package model;

import java.io.File;
import java.util.Observable;

public class Files extends Observable{
	
	File[] files = null;

	public File[] getFiles() {
		return files;
	}

	public void setFiles(File[] files) {
		this.files = files;
		setChanged();
        notifyObservers(this.files);
	}

	
	
	
}
