package model;

import java.io.File;
import java.util.Arrays;

public class Files {
	
	File[] files;

	public File[] getFiles() {
		return files;
	}

	public void setFiles(File[] files) {
		this.files = files;
	}

	@Override
	public String toString() {
		String s = "";
		for (int i = 0; i < files.length; i++) {
			s += files[i].getAbsolutePath() + " /n ";
		}
		return s;
	}
	
	
	
}
